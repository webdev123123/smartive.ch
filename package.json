{
  "name": "smartive-website-2021",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.js",
    "start": "next start",
    "lint": "next lint --dir src && prettier --check src",
    "analyze-bundle": "ANALYZE=true npm run build",
    "test": "node test/check-page-errors.js",
    "test:ci": "next dev & node test/check-page-errors.js",
    "deploy:preview": "vercel --token $VERCEL_TOKEN --yes --scope smartive > deployment-url.txt",
    "deploy:production": "vercel --token $VERCEL_TOKEN --confirm --scope smartive --prod",
    "prismic:generate": "graphql-codegen --config codegen.yml"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@notionhq/client": "2.2.3",
    "@prismicio/next": "^1.0.2",
    "@smartive/guetzli": "^2.9.0",
    "@xstate/fsm": "2.0.0",
    "@xstate/react": "3.2.1",
    "blobs": "2.2.0",
    "charts.css": "1.0.0",
    "dayjs": "1.11.7",
    "eslint-config-next": "^13.1.0",
    "framer-motion": "10.8.4",
    "glider-js": "^1.7.7",
    "graphql": "16.6.0",
    "graphql-request": "5.2.0",
    "image-size": "1.0.2",
    "js-confetti": "0.11.0",
    "mermaid": "10.0.2",
    "motion": "10.15.5",
    "next": "13.2.4",
    "next-plausible": "3.7.2",
    "node-fetch": "3.3.1",
    "prism-react-renderer": "1.3.5",
    "prismic-reactjs": "1.3.4",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "9.4.3",
    "react-mailchimp-subscribe": "2.1.3",
    "react-photo-gallery-next": "8.0.1",
    "rokka": "3.13.3",
    "scrollex": "^2.0.1",
    "sharp": "0.31.3",
    "xstate": "4.37.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.5.13",
    "@next/bundle-analyzer": "13.2.4",
    "@smartive/eslint-config": "3.1.1",
    "@smartive/prettier-config": "3.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/mermaid": "9.1.0",
    "@types/node": "18.15.5",
    "@types/puppeteer": "5.4.7",
    "@types/puppeteer-core": "5.4.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "eslint": "8.36.0",
    "next-sitemap": "4.0.6",
    "playwright": "1.31.2",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "prettier": "2.8.6",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2"
  }
}
