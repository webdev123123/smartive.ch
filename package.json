{
  "name": "smartive-website-2021",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "develop": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.js",
    "start": "next start",
    "lint": "next lint --dir src && prettier --check src",
    "analyze-bundle": "ANALYZE=true npm run build",
    "test": "node test/check-page-errors.js",
    "test:ci": "next dev & node test/check-page-errors.js",
    "deploy:preview": "vercel --token $VERCEL_TOKEN --yes --scope smartive > deployment-url.txt",
    "deploy:production": "vercel --token $VERCEL_TOKEN --confirm --scope smartive --prod",
    "prismic:generate": "graphql-codegen --config codegen.yml"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@notionhq/client": "2.2.8",
    "@prismicio/next": "^1.0.2",
    "@smartive/guetzli": "2.9.0",
    "@xstate/fsm": "2.1.0",
    "@xstate/react": "3.2.2",
    "blobs": "2.2.0",
    "charts.css": "1.0.0",
    "dayjs": "1.11.9",
    "eslint-config-next": "^13.1.0",
    "framer-motion": "10.12.22",
    "glider-js": "^1.7.7",
    "graphql": "16.7.1",
    "graphql-request": "6.1.0",
    "image-size": "1.0.2",
    "js-confetti": "0.11.0",
    "mermaid": "10.2.4",
    "motion": "10.16.2",
    "next": "13.4.10",
    "next-plausible": "3.10.1",
    "node-fetch": "3.3.1",
    "prism-react-renderer": "2.0.6",
    "prismic-reactjs": "1.3.4",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "9.5.2",
    "react-mailchimp-subscribe": "2.1.3",
    "react-photo-gallery-next": "8.0.1",
    "rokka": "3.14.1",
    "scrollex": "^2.0.1",
    "sharp": "0.32.3",
    "xstate": "4.38.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@next/bundle-analyzer": "13.4.10",
    "@smartive/eslint-config": "3.2.0",
    "@smartive/prettier-config": "3.0.0",
    "@tailwindcss/forms": "0.5.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/mermaid": "9.1.0",
    "@types/node": "18.16.19",
    "@types/puppeteer": "5.4.7",
    "@types/puppeteer-core": "5.4.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "next-sitemap": "4.1.8",
    "playwright": "1.36.1",
    "postcss": "8.4.26",
    "postcss-import": "15.1.0",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "wait-on": "^7.0.1"
  }
}
