{
  "name": "smartive-website-2021",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.js",
    "start": "next start",
    "lint": "next lint --dir src && prettier --check src",
    "analyze-bundle": "ANALYZE=true npm run build",
    "test": "node test/check-page-errors.js",
    "test:ci": "next dev & node test/check-page-errors.js",
    "deploy:preview": "vercel --token $VERCEL_TOKEN --yes --scope smartive > deployment-url.txt",
    "deploy:production": "vercel --token $VERCEL_TOKEN --confirm --scope smartive --prod",
    "prismic:generate": "graphql-codegen --config codegen.yml"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@notionhq/client": "2.2.2",
    "@prismicio/next": "0.1.9",
    "@smartive/guetzli": "2.6.0",
    "@tryghost/content-api": "1.11.5",
    "@xstate/fsm": "2.0.0",
    "@xstate/react": "3.0.1",
    "blobs": "2.2.0",
    "charts.css": "0.9.0",
    "dayjs": "1.11.6",
    "eslint-config-next": "13.0.6",
    "framer-motion": "7.6.18",
    "glider-js": "^1.7.7",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "image-size": "1.0.2",
    "js-confetti": "0.11.0",
    "mermaid": "^9.1.7",
    "motion": "10.14.3",
    "next": "12.3.4",
    "next-plausible": "3.6.4",
    "node-fetch": "3.3.0",
    "prism-react-renderer": "1.3.5",
    "prismic-reactjs": "1.3.4",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.1",
    "react-mailchimp-subscribe": "2.1.3",
    "react-photo-gallery-next": "^8.0.1",
    "rokka": "^3.9.0",
    "scrollex": "2.0.0",
    "sharp": "0.31.2",
    "xstate": "4.34.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.8.3",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.5.8",
    "@next/bundle-analyzer": "12.3.4",
    "@smartive/eslint-config": "3.1.1",
    "@smartive/prettier-config": "3.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.8",
    "@types/mermaid": "9.1.0",
    "@types/node": "18.11.10",
    "@types/puppeteer": "5.4.7",
    "@types/puppeteer-core": "5.4.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/tryghost__content-api": "1.3.11",
    "autoprefixer": "10.4.13",
    "eslint": "8.29.0",
    "next-sitemap": "3.1.32",
    "playwright": "1.28.1",
    "postcss": "8.4.19",
    "postcss-import": "15.0.1",
    "prettier": "2.8.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  }
}
