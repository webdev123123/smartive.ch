    "@xstate/react": "2.0.1",
    "blobs": "2.2.0",
    "body-scroll-lock": "3.1.5",
    "charts.css": "0.9.0",
    "dayjs": "1.11.0",
    "eslint-config-next": "12.1.4",
    "framer-motion": "6.2.9",
    "graphql": "16.3.0",
