{
  "name": "@smartive/guetzli",
  "version": "1.0.0",
  "description": "smartive Component Library created with Typescript React and styled with TailwindCSS.",
  "author": "smartive AG",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.module.js",
      "script": "./dist/index.modern.js"
    },
    "./config": "./dist/config.js",
    "./styles/": "./styles/"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "styles/guetzli.css",
    "tailwind.config.js",
    "README.md"
  ],
  "scripts": {
    "build": "microbundle --no-compress --jsx React.createElement --jsxImportSource React && microbundle -i src/tailwind-config.ts -o dist/config.js --no-pkg-main -f umd",
    "develop": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "generate-index-files": "cti create ./src --fileexcludes tailwind-config.ts --excludes types --withoutbackup",
    "lint": "prettier --check src && eslint src",
    "lint:fix": "prettier --write src && eslint src --fix",
    "publish": "semantic-release --dry-run"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/gitlab"
    ]
  },
  "dependencies": {
    "blobs": "^2.2.0",
    "body-scroll-lock": "^3.1.5",
    "deepmerge": "^4.2.2",
    "framer-motion": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-laag": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/gitlab": "^7.0.4",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@smartive/eslint-config": "^2.0.0",
    "@smartive/prettier-config": "^3.0.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "create-ts-index": "^1.13.6",
    "eslint": "^7.24.0",
    "microbundle": "^0.13.0",
    "postcss": "^8.2.10",
    "postcss-import": "^14.0.1",
    "prettier": "^2.2.1",
    "semantic-release": "^18.0.1",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.1.4"
  }
}
